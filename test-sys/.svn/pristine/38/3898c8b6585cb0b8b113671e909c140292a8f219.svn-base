    SELECT
    <choose>
      <when test="end != 0">
        fu.*,wo.name
      </when>
      <otherwise>
        COUNT(*) TOTAL
      </otherwise>
    </choose>
    FROM front_user fu LEFT JOIN windy_org wo
    ON fu.org_id = wo.id
    WHERE fu.org_id = #{orgId}
    <if test="end != 0">
      LIMIT #{start},#{end}
    </if>
  </select>
  
