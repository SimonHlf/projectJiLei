<%@page pageEncoding="UTF-8" %>
<div class="widget model_widget">
    <div class="widget-body">
        <div id="horizontal-form">
            <form class="form-horizontal" method="post" id="editDisrelationshipForm">
                <input type="hidden" name="id" value="${DisRate.id}">
                <div >
                    <div class="form-group">
                        <label class="col-sm-2 control-label no-padding-right">分销级别名称</label>
                        <div class="col-sm-9">
                            <input class="form-control" id="name" name="name" value="${DisRate.name}" placeholder="分销级别名称" required>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label no-padding-right">类型</label>
                        <div class="col-sm-9">
                            <input class="form-control" id="type" name="type" value="${DisRate.type}" placeholder="类型" required>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label no-padding-right">分销费率值</label>
                        <div class="col-sm-9">
                            <input class="form-control" id="rateValue" name="rateValue" value="${DisRate.rateValue}" placeholder="分销费率值" required>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label no-padding-right">描述</label>
                        <div class="col-sm-9">
                            <input class="form-control" id="rateDescribe" name="rateDescribe" value="${DisRate.rateDescribe}" placeholder="描述">
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>