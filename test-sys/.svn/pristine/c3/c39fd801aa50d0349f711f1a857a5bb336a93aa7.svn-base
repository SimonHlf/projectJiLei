<%@page pageEncoding="utf-8" %>
<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<div class="widget model_widget">
    <div class="widget-body">
        <div id="horizontal-form">
            <form class="form-horizontal" method="post" id="addDisRateForm">
                <div >
                    <div class="form-group">
                        <label class="col-sm-2 control-label no-padding-right">试卷名称</label>
                        <div class="col-sm-9">
                            <input class="form-control" id="name" name="name" readonly="readonly" value="第一次考试" placeholder="分类名称" required>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label no-padding-right">试卷类型</label>
                        <div class="col-sm-9">
                            <input class="form-control" id="name" name="name" readonly="readonly" value="考试" placeholder="分类名称" required>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label no-padding-right">试卷总分</label>
                        <div class="col-sm-9">
                            <input class="form-control" id="name" name="name" readonly="readonly" value="100" placeholder="分类名称" required>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label no-padding-right">总得分</label>
                        <div class="col-sm-9">
                            <input class="form-control" id="name" name="name" readonly="readonly" value="80" placeholder="分类名称" required>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label no-padding-right">单选题答对的题数</label>
                        <div class="col-sm-9">
                            <input class="form-control" id="name" name="name" readonly="readonly" value="12" placeholder="分类名称" required>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label no-padding-right">多选题题答对的题数</label>
                        <div class="col-sm-9">
                            <input class="form-control" id="name" name="name" readonly="readonly" value="8" placeholder="分类名称" required>
                        </div>
                    </div>
                     <div class="form-group">
                        <label class="col-sm-2 control-label no-padding-right">判断题题题答对的题数</label>
                        <div class="col-sm-9">
                            <input class="form-control" id="name" name="name" readonly="readonly" value="10" placeholder="分类名称" required>
                        </div>
                    </div>
                    <div class="widget-buttons">
                			<button type="button" onclick="fanhui()" class="btn btn-default"> 返回</button>
            		</div>
                </div>
            </form>
        </div>
    </div>
</div>
