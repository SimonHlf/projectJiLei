    //柱状图
    $(function () {
        var lableList = "${lableList}";
        var totalList = '${totalList}';
        var passList = '${passList}';
        var myChart = echarts.init(document.getElementById('BarEcharts'));
        option = {
            title: {
                text: '近${lableList.size()}场考试及格人数'
            },
            tooltip: {
                trigger: 'axis'
            },
            legend: {
                data: ['蒸发量', '降水量']
            },
            toolbox: {
                show: true,
                feature: {
                    dataView: {show: false, readOnly: false},
                    magicType: {show: false, type: ['line', 'bar']},
                    restore: {show: false},
                    saveAsImage: {show: false}
                }
            },
            calculable: true,
            xAxis: [
                {
                    type: 'category',
                    data: eval('(' + lableList + ')')/*['2017-08','2017-09','2017-10','2017-11','2017-12','2018-01']*/
                }
            ],
            yAxis: [
                {
                    type: 'value'
                }
            ],
            series: [
                {
                    name: '及格人数',
                    type: 'bar',
                    data: eval('(' + passList + ')')/*[ 23.2, 25.6, 36.7, 30.6, 12.2, 32.6]*/,
                    markPoint: {
                        data: [
                            {type: 'max', name: '最大值'},
                            {type: 'min', name: '最小值'}
                        ]
                    },
                    markLine: {
                        data: [
                            {type: 'average', name: '平均值'}
                        ]
                    }
                },
                {
                    name: '应考人数',
                    type: 'bar',
                    data: eval('(' + totalList + ')')/*[80.7, 90.7, 95.6, 82.2, 84.7, 81.8]*/,
                    markPoint: {
                        data: [
                            {name: '年最高', value: 182.2, xAxis: 7, yAxis: 183},
                            {name: '年最低', value: 2.3, xAxis: 11, yAxis: 3}
                        ]
                    },
                    markLine: {
                        data: [
                            {type: 'average', name: '平均值'}
                        ]
                    }
                }
            ]
        };
        myChart.setOption(option);
    })


    //折线图
    $(function () {
       var lineNameList = "${lineNameList}";
       var scoreList = '${scoreList}';
        var myChart = echarts.init(document.getElementById('PieEcharts'));
        option = {
            title: {
                text: '近${lableList.size()}场考试优秀人数'
            },
            tooltip: {
                trigger: 'axis'
            },
            toolbox: {
                show: false,
                feature: {
                    dataZoom: {
                        yAxisIndex: 'none'
                    },
                    dataView: {readOnly: false},
                    magicType: {type: ['line', 'bar']},
                    restore: {},
                    saveAsImage: {}
                }
            },
            xAxis: {
                type: 'category',
                boundaryGap: false,
                data: eval('('+lineNameList+')')/*['周一', '周二', '周三']*/
            },
            yAxis: {
                type: 'value',
                axisLabel: {
                    formatter: '{value}'
                }
            },
            series: [
                {
                    name: '',
                    type: 'line',
                    data: eval('('+scoreList+')')/*[30, 11, 15]*/,
                    markPoint: {
                        data: [
                            {type: 'max', name: '最大值'},
                            {type: 'min', name: '最小值'}
                        ]
                    },
                    markLine: {
                        data: [
                            {type: 'average', name: '平均值'}
                        ]
                    }
                }
            ]
        };
        myChart.setOption(option);
    })
</script>
