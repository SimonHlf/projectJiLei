<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="connection.url">jdbc:mysql://localhost:3306/examination</property>
        <property name="connection.driver_class">com.mysql.jdbc.Driver</property>
        <mapping class="cn.windy.module.admin.hibernate.model.DataPermission"/>
        <mapping class="cn.windy.module.admin.hibernate.model.Item"/>
        <mapping class="cn.windy.module.admin.hibernate.model.Menu"/>
        <mapping class="cn.windy.module.admin.hibernate.model.Org"/>
        <mapping class="cn.windy.module.admin.hibernate.model.OrgsUsers"/>
        <mapping class="cn.windy.module.admin.hibernate.model.Role"/>
        <mapping class="cn.windy.module.admin.hibernate.model.RolesMenus"/>
        <mapping class="cn.windy.module.admin.hibernate.model.Setting"/>
        <mapping class="cn.windy.module.admin.hibernate.model.User"/>
        <mapping class="cn.windy.module.admin.hibernate.model.UsersRoles"/>


    </session-factory>
</hibernate-configuration>